{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["static", "^build"]
    },
    "clean": {
      "cache": false
    },
    "clean:deep": {
      "cache": false
    },
    "doctor:fix": {
      "cache": false
    },
    "local": {
      "dependsOn": ["^local"],
      "persistent": true
    },
    "static": {
      "cache": false
    },
    "static:fix": {
      "cache": false
    }
  }
}